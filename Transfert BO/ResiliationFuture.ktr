    <sql>SELECT&#xd;&#xa;  t.initiativeCode as &#x27;typeResil&#x27;&#xd;&#xa;, t.code as &#x27;motifResil&#x27;&#xd;&#xa;, t.creationDate as &#x27;dateCreationResil&#x27;&#xd;&#xa;, t.date as &#x27;dateResil&#x27;&#xd;&#xa;, true as &#x27;enCascade&#x27;&#xd;&#xa;, true as &#x27;remboursement&#x27;&#xd;&#xa;, true as &#x27;penalite&#x27;&#xd;&#xa;, true as &#x27;fraisResiliation&#x27;&#xd;&#xa;, &#x27;Migration Donn&#xe9;es BO&#x27; as &#x27;commentaire&#x27;&#xd;&#xa;&#xd;&#xa;, p.productKey as &#x27;referenceContrat&#x27;&#xd;&#xa;FROM termination t&#xd;&#xa;  INNER JOIN product p on &#x28;p.termination_idFK &#x3d; t.id and &#x28;p.catalogReference like &#x22;avfw.&#x25;&#x22; or p.catalogReference like &#x22;mobileprotection.&#x25;&#x22;&#x29;&#x29;&#xd;&#xa;where t.date &#x3e;&#x3d; NOW&#x28;&#x29;&#xd;&#xa;  and t.state &#x3c;&#x3e; &#x27;AN&#x27;&#xd;&#xa;order by t.date</sql>
