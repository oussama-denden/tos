    <sql>SELECT&#xd;&#xa;  p.productKey as &#x27;referenceContrat&#x27;&#xd;&#xa;, p.catalogReference as &#x27;referenceProduit&#x27;&#xd;&#xa;, IF&#x28;&#x28;COALESCE&#x28;p.customLabel, &#x27;&#x27;&#x29; like &#x27;Personnaliser&#x25;&#x27;&#x29;, p.label,COALESCE&#x28;p.customLabel, p.label&#x29;&#x29; as &#x27;titre&#x27;&#xd;&#xa;, p.state&#xd;&#xa;, p.creationDate as &#x27;datePreparation&#x27;&#xd;&#xa;, p.creationDate as &#x27;dateValidation&#x27;&#xd;&#xa;, p.changeDate as &#x27;dateDerniereFacturation&#x27;&#xd;&#xa;, p.paymentDate as &#x27;dateDebutFacturation&#x27;&#xd;&#xa;, p.duration as &#x27;periodicite&#x27;&#xd;&#xa;, &#x28;CASE WHEN p.renewMode &#x3d; &#x27;AUTO_RENEWAL&#x27; THEN null ELSE p.duration END&#x29; as &#x27;duree&#x27;&#xd;&#xa;, &#x28;CASE WHEN p.renewMode &#x3d; &#x27;AUTO_RENEWAL&#x27; THEN null ELSE p.dueDate END&#x29; as &#x27;dateFinDuree&#x27;&#xd;&#xa;&#xd;&#xa;, p.paymentMethod&#xd;&#xa;, p.RUM&#xd;&#xa;&#xd;&#xa;, hInstallation.handleKey as &#x27;idAdrLivraison&#x27;&#xd;&#xa;, hFacturation.handleKey as &#x27;idAdrFacturation&#x27;&#xd;&#xa;&#xd;&#xa;, o.orderKey as &#x27;referenceCommande&#x27;&#xd;&#xa;&#xd;&#xa;, oi.htPrice as &#x27;montant&#x27;&#xd;&#xa;, &#x27;&#x27; as &#x27;referenceTarif&#x27;&#xd;&#xa;&#xd;&#xa;, &#x27;1&#x27; as &#x27;numEC&#x27;&#xd;&#xa;, true as &#x27;remboursable&#x27;&#xd;&#xa;, &#x27;DATE_ANNIVERSAIRE&#x27; as &#x27;modeFacturation&#x27;&#xd;&#xa;, &#x27;SERVICE&#x27; as &#x27;typeProduit&#x27;&#xd;&#xa;, &#x27;S&#x27; as &#x27;typeTVA&#x27;&#xd;&#xa;&#xd;&#xa;FROM product p&#xd;&#xa;  INNER JOIN join_order_product jop ON &#x28;jop.product_idFK &#x3d; p.id&#x29;&#xd;&#xa;  INNER JOIN order_info o 			ON &#x28;o.id &#x3d; jop.order_idFK&#x29;&#xd;&#xa;  INNER JOIN order_item_group oig   ON &#x28;oig.order_idFK &#x3d; o.id&#x29;&#xd;&#xa;  INNER JOIN order_item oi 			ON &#x28;oi.group_idFK &#x3d; oig.id&#x29;&#xd;&#xa;  LEFT OUTER JOIN handle hInstallation 	ON &#x28;hInstallation.id &#x3d; o.installationHandle_idFK&#x29;&#xd;&#xa;  LEFT OUTER JOIN handle hFacturation 	ON &#x28;hFacturation.id &#x3d; o.invoiceHandle_idFK&#x29;&#xd;&#xa;&#xd;&#xa;-- where &#x28;p.catalogReference like &#x22;avfw.&#x25;&#x22; or p.catalogReference like &#x22;mobileprotection.&#x25;&#x22;&#x29;&#xd;&#xa;where p.catalogReference like &#x22;avfw.&#x25;&#x22;&#xd;&#xa;and p.state NOT IN &#x28;&#x27;AD&#x27;, &#x27;PE&#x27;&#x29;&#xd;&#xa;and p.type &#x3c;&#x3e; &#x27;TRIAL&#x27;&#xd;&#xa;order by p.productKey&#xd;&#xa;LIMIT 2000&#x3b;</sql>
    <limit>0</limit>
