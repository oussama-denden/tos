    <sql>SELECT&#xd;&#xa;  o.orderKey&#xd;&#xa;, o.orderDate&#xd;&#xa;, o.creationDate&#xd;&#xa;, o.changeDate&#xd;&#xa;, p.productKey&#xd;&#xa;&#xd;&#xa;, &#x27;Migration donn&#xe9;es BO&#x27; as &#x27;commentaire&#x2f;user&#x27;&#xd;&#xa;, true as &#x27;conserverAncienneReduction&#x27;&#xd;&#xa;, false as &#x27;fraisResil&#x27;&#xd;&#xa;, false as &#x27;fraisCrea&#x27;&#xd;&#xa;, false as &#x27;fraisMig&#x27;&#xd;&#xa;, false as &#x27;penalite&#x27;&#xd;&#xa;, true as &#x27;remboursement&#x27;&#xd;&#xa;, o.type as &#x27;typeAvenant&#x27;&#xd;&#xa;&#xd;&#xa;, 1 as &#x27;numEC&#x27;&#xd;&#xa;FROM order_info o&#xd;&#xa;  INNER JOIN join_order_product jop on &#x28;jop.order_idFK &#x3d; o.id&#x29;&#xd;&#xa;  INNER JOIN product p on &#x28;jop.product_idFK &#x3d; p.id&#x29;&#xd;&#xa;where o.type NOT IN &#x28;&#x27;NORMAL&#x27;, &#x27;CONFIRMATION&#x27;&#x29;&#xd;&#xa;and o.id in &#x28;&#xd;&#xa;    select oig.order_idFK from order_item_group oig where oig.id in &#x28;&#xd;&#xa;        select oi.group_idFK&#xd;&#xa;          from order_item oi&#xd;&#xa;         where &#x28;oi.itemCode like &#x22;avfw.&#x25;&#x22; or oi.itemCode like &#x22;mobileprotection.&#x25;&#x22; &#x29;&#x29;&#x29;&#xd;&#xa;and o.state NOT IN &#x28;&#x27;AD&#x27;, &#x27;PE&#x27;, &#x27;AN&#x27;&#x29;&#xd;&#xa;order by p.productKey, o.orderDate&#x3b;&#xd;&#xa;</sql>
