    <sql>SELECT&#xd;&#xa;  o.orderKey&#xd;&#xa;, o.orderDate&#xd;&#xa;, o.state&#xd;&#xa;, o.createdBy&#xd;&#xa;, p.partnerKey as &#x27;codePartenaire&#x27;&#xd;&#xa;, c.customerKey as &#x27;customerReference&#x27;&#xd;&#xa;&#xd;&#xa;, hShipping.handleKey as &#x27;adresseLivraison&#x27;&#xd;&#xa;, hShipping.creationDate as &#x27;adresseLivraisonDateCreation&#x27;&#xd;&#xa;, hInvoice.handleKey as &#x27;adresseFacturation&#x27;&#xd;&#xa;, hInvoice.creationDate as &#x27;adresseFacturationDateCreation&#x27;&#xd;&#xa;, hInstallation.handleKey as &#x27;adresse&#x27;&#xd;&#xa;, hInstallation.creationDate as &#x27;adresseDateCreation&#x27;&#xd;&#xa;, &#x27;00&#x27; as &#x27;tva&#x27;&#xd;&#xa;&#xd;&#xa;, o.creationDate&#xd;&#xa;, o.changeDate&#xd;&#xa;, &#x27;BackOffice&#x28;Migration&#x29;&#x27; as &#x27;canal&#x27;&#xd;&#xa;FROM order_info o&#xd;&#xa;  INNER JOIN order_item_group oig ON &#x28;o.id &#x3d; oig.order_idFk&#x29;&#xd;&#xa;  INNER JOIN order_item oi on &#x28;oi.group_idFK &#x3d; oig.id&#x29;&#xd;&#xa;  LEFT OUTER JOIN partner p on &#x28;p.id &#x3d; o.partner_idFK&#x29;&#xd;&#xa;  LEFT OUTER JOIN customer c on &#x28;c.id &#x3d; o.customer_idFK&#x29;&#xd;&#xa;  LEFT OUTER JOIN handle hInvoice on &#x28;hInvoice.id &#x3d; o.invoiceHandle_idFK&#x29;&#xd;&#xa;  LEFT OUTER JOIN handle hInstallation on &#x28;hInstallation.id &#x3d; o.installationHandle_idFK&#x29;&#xd;&#xa;  LEFT OUTER JOIN handle hShipping on &#x28;hShipping.id &#x3d; o.shippingHandle_idFK&#x29;&#xd;&#xa;WHERE oi.itemCode like &#x22;avfw.&#x25;&#x22;&#xd;&#xa;order by rand&#x28;&#x29;&#xd;&#xa;</sql>
